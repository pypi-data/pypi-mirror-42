#!python
import sys
import os


args = ""
for i in range(1,len(sys.argv)):
    args += " " + sys.argv[i]

if not os.access("/etc/bash_completion.d/rave_compl.bash", os.R_OK):
    command = "cp /lib/python2.7/site-packages/ravegen/rave_compl.bash /etc/bash_completion.d/rave_compl.bash")

command = "python /lib/python2.7/site-packages/ravegen/main.py " + args

os.system(command)
